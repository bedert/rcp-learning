<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.example.addressbook.editor.openaddress"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:com.example.addressbook.AddressList">
         <command
               commandId="com.example.addressbook.editor.openaddress"
               label="%command.label"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.example.addressbook.editor.AddressEditor"
            default="false"
            id="com.example.addressbook.editor.addresseditor"
            name="%editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.example.addressbook.editor.handler.OpenAddressHandler"
            commandId="com.example.addressbook.editor.openaddress">
      </handler>
   </extension>

</plugin>
